\r\n        },\r\n        series: [\r\n          {\r\n            name: '入库',\r\n            type: 'bar',\r\n            data: this.mapData.inbound\r\n          },\r\n          {\r\n            name: '出库',\r\n            type: 'bar',\r\n            data: this.mapData.outbound\r\n          },\r\n        ],\r\n      };\r\n      this.chart.setOption(option);\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}