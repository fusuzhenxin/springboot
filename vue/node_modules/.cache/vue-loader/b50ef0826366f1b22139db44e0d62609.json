s\">\r\n          <div>\r\n            <el-image style=\"width: 120px; height: 120px\" :src=\"resultdata.picture\" fit=\"fill\"></el-image>\r\n          </div>\r\n        </div>\r\n        <div :style=\"fun_infos\">\r\n          <h2>\r\n            <a>{{ ruleForm.name }}<span class=\"xz-tip\">学生</span></a>\r\n          </h2>\r\n          <p>{{ ruleForm.valueProvinceCity }} | {{ ruleForm.education }} | {{ ruleForm.sex }}</p>\r\n          <p>{{ ruleForm.phonenum }} | {{ ruleForm.email }}</p>\r\n          <el-button type=\"primary\" icon=\"el-icon-search\" style=\"margin-bottom: 10px\" @click=\"information_modify()\">编辑\r\n          </el-button>\r\n        </div>\r\n        <div :style=\"alter_funinfo\" style=\"width:450px;margin-right: auto;margin-left: auto;margin-top: 15px\">\r\n          <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"100px\" class=\"demo-ruleForm\">\r\n            <el-form-item label=\"学号\" prop=\"stu_id\">\r\n              <el-input v-model=\"ruleForm.stu_id\" readonly style=\"width: 300px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"姓名\" prop=\"name\">\r\n              <el-input v-model=\"ruleForm.name\" resize=\"\" style=\"width: 300px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"性别\" prop=\"sex\" required>\r\n              <el-radio-group v-model=\"ruleForm.sex\">\r\n                <el-radio label=\"男\"></el-radio>\r\n                <el-radio label=\"女\"></el-radio>\r\n              </el-radio-group>\r\n            </el-form-item>\r\n            <el-form-item label=\"工作省市\" prop=\"valueProvinceCity\">\r\n              <el-input v-model=\"ruleForm.valueProvinceCity\" style=\"width: 200px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"学历\" required>\r\n              <el-select v-model=\"ruleForm.education\" placeholder=\"请选择活动区域\">\r\n                <el-option label=\"小学\" value=\"1\"></el-option>\r\n                <el-option label=\"专科\" value=\"2\"></el-option>\r\n                <el-option label=\"本科\" value=\"3\"></el-option>\r\n                <el-option label=\"硕士\" value=\"4\"></el-option>\r\n                <el-option label=\"博士\" value=\"5\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"手机\" required>\r\n              <el-input v-model=\"ruleForm.phonenum\" style=\"width: 300px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"邮箱\" required>\r\n              <el-input v-model=\"ruleForm.email\" style=\"width: 300px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"submitForm()\">立即创建</el-button>\r\n              <el-button @click=\"information_modify_cancel\">取消</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n      </el-header>\r\n      <el-main class=\"el-main\">\r\n        <div class=\"tit\">教育经历</div>\r\n        <div style=\"width:450px;margin-right: auto;margin-left: auto;margin-top: 15px\">\r\n          <el-form label-width=\"130px\">\r\n            <el-form-item label=\"学校\">\r\n              <el-input v-model=\"resultdata.school\" readonly value=\"\" style=\"width: 200px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"专业\">\r\n              <el-input v-model=\"resultdata.major\" readonly value=\"\" style=\"width: 200px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"入学时间\">\r\n              <el-date-picker type=\"date\" placeholder=\"选择日期\" readonly\r\n                              v-model=\"resultdata.inschool\"></el-date-picker>\r\n            </el-form-item>\r\n            <el-form-item label=\"毕业时间\" required>\r\n              <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"resultdata.outschool\"></el-date-picker>\r\n            </el-form-item>\r\n            <el-form-item label=\"在校经历\" required>\r\n              <el-input type=\"textarea\" v-model=\"resultdata.experience\" style=\"width: 400px\"></el-input>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n        <div class=\"tit\">项目经历</div>\r\n        <div style=\"width:450px;margin-right: auto;margin-left: auto;margin-top: 15px\">\r\n          <el-form label-width=\"130px\">\r\n            <el-form-item label=\"项目名称\" required>\r\n              <el-input v-model=\"resultdata.projectname\" style=\"width: 300px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"负责内容\" required>\r\n              <el-input v-model=\"resultdata.projectcharge\" style=\"width: 300px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"项目描述\" required>\r\n              <el-input type=\"textarea\" v-model=\"resultdata.projectdescribe\" style=\"width: 400px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"项目成绩(选填)\">\r\n              <el-input type=\"textarea\" v-model=\"resultdata.projectgrades\" style=\"width: 400px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"项目链接(选填)\">\r\n              <el-input type=\"textarea\" v-model=\"resultdata.projectlink\" style=\"width: 400px\"></el-input>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n        <div class=\"tit\">实习经历</div>\r\n        <div style=\"width:450px;margin-right: auto;margin-left: auto;margin-top: 15px\">\r\n          <el-form label-width=\"130px\">\r\n            <el-form-item label=\"所在行业\" required>\r\n              <el-input v-model=\"resultdata.industry\" style=\"width: 300px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"实习开始时间\" required>\r\n              <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"resultdata.inintershiptime\"></el-date-picker>\r\n            </el-form-item>\r\n            <el-form-item label=\"实习结束时间\" required>\r\n              <el-date-picker type=\"date\" placeholder=\"选择日期\" v-model=\"resultdata.outintershiptime\"></el-date-picker>\r\n            </el-form-item>\r\n            <el-form-item label=\"实习职位\" required>\r\n              <el-input v-model=\"resultdata.intershipposts\" style=\"width: 300px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"工作内容\" required>\r\n              <el-input type=\"textarea\" v-model=\"resultdata.intershipwork\" style=\"width: 400px\"></el-input>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n        <div class=\"tit\">求职期望</div>\r\n        <div style=\"width:450px;margin-right: auto;margin-left: auto;margin-top: 15px\">\r\n          <el-form label-width=\"130px\">\r\n            <el-form-item label=\"期望职位\" required>\r\n              <el-input v-model=\"resultdata.wishpost\" style=\"width: 300px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"薪资要求\" required>\r\n              <el-input v-model=\"resultdata.salary\" style=\"width: 200px\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"求职状态\">\r\n              <el-select v-model=\"resultdata.jobstatus\" placeholder=\"请选择\">\r\n                <el-option label=\"在职\" value=\"1\"></el-option>\r\n                <el-option label=\"求职\" value=\"2\"></el-option>\r\n                <el-option label=\"暂不考虑\" value=\"3\"></el-option>\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"onSubmit()\">保存</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n      </el-main>\r\n    </el-container>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: 'FontOutbound',\r\n  data() {\r\n    return {\r\n      resultdata: [],\r\n      fun_infos: {display: ''},\r\n      alter_funinfo: {display: 'none'},\r\n      ruleForm: {\r\n        stu_id: '',\r\n        name: '',\r\n        sex: '',\r\n        valueProvinceCity: '',\r\n        education: '',\r\n        phonenum: '',\r\n        email: ''\r\n      },\r\n      rules: {\r\n        stu_id: [],\r\n        name: [\r\n          {required: true, message: '请输入姓名', trigger: 'blur'},\r\n          {min: 2, max: 4, message: '长度在 2 到 4 个字符', trigger: 'blur'}\r\n        ],\r\n        sex: [\r\n          {required: true, message: '请选择性别', trigger: 'change'}\r\n        ],\r\n        valueProvinceCity: [\r\n          {required: true, message: '请填写省市', trigger: 'blur'}\r\n        ],\r\n        education: [\r\n          {required: true, message: '请选择你的学历', trigger: 'change'}\r\n        ],\r\n        phonenum: [\r\n          {required: true, message: '请填写电话', trigger: 'blur'},\r\n          {min: 11, max: 11, message: '长度在11位数字', trigger: 'blur'}\r\n        ],\r\n        email: [\r\n          {required: true, message: '请填写邮箱', trigger: 'blur'}\r\n        ]\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      this.alter_funinfo = {display: 'none'}\r\n      this.fun_infos = {display: ''}\r\n    },\r\n    information_modify() {\r\n      this.fun_infos = {display: 'none'}\r\n      this.alter_funinfo = {display: ''}\r\n    },\r\n    information_modify_cancel() {\r\n      this.ruleForm.stu_id = this.resultdata.passedUsername\r\n      this.ruleForm.name = this.resultdata.name\r\n      this.ruleForm.sex = this.resultdata.gender\r\n      this.ruleForm.valueProvinceCity = this.resultdata.workplace\r\n      this.ruleForm.education = this.resultdata.degree\r\n      this.ruleForm.phonenum = this.resultdata.phone\r\n      this.ruleForm.email = this.resultdata.email\r\n      this.alter_funinfo = {display: 'none'}\r\n      this.fun_infos = {display: ''}\r\n    },\r\n    onSubmit() {\r\n      this.$axios\r\n          .put('/myself/update', {\r\n            id: this.resultdata.id,\r\n            name: this.resultdata.name,\r\n            gender: this.ruleForm.sex,\r\n            workplace: this.ruleForm.valueProvinceCity,\r\n            degree: this.ruleForm.education,\r\n            phone: this.ruleForm.phonenum,\r\n            email: this.ruleForm.email,\r\n            school: this.resultdata.school,\r\n            major: this.resultdata.major,\r\n            inschool: this.resultdata.inschool,\r\n            outschool: this.resultdata.outschool,\r\n            experience: this.resultdata.experience,\r\n            projectname: this.resultdata.projectname,\r\n            projectcharge: this.resultdata.projectcharge,\r\n            projectdescribe: this.resultdata.projectdescribe,\r\n            projectgrades: this.resultdata.projectgrades,\r\n            projectlink: this.resultdata.projectlink,\r\n            industry: this.resultdata.industry,\r\n            inintershiptime: this.resultdata.inintershiptime,\r\n            outintershiptime: this.resultdata.outintershiptime,\r\n            intershipposts: this.resultdata.intershipposts,\r\n            intershipwork: this.resultdata.intershipwork,\r\n            wishpost: this.resultdata.wishpost,\r\n            salary: this.resultdata.salary,\r\n            jobstatus: this.resultdata.jobstatus\r\n          })\r\n          .then(successResponse => {\r\n            // console.log(successResponse)\r\n            this.$notify({\r\n              title: '成功',\r\n              message: '已成功保存简历信息',\r\n              type: 'success'\r\n            })\r\n          })\r\n          .catch(failResponse => {\r\n            // console.error(failResponse)\r\n            this.$notify({\r\n              title: '错误',\r\n              message: '不能正确保存简历信息',\r\n              type: 'warning'\r\n            })\r\n          })\r\n    }\r\n  },\r\n  mounted() {\r\n    const userData = localStorage.getItem('user')\r\n    const userObject = JSON.parse(userData)\r\n    // console.log(userObject.username)\r\n    this.$axios\r\n        .get('/myself/search', {\r\n          params: {\r\n            passedUsername: userObject.username\r\n          }\r\n        })\r\n        .then(successResponse => {\r\n              // console.log(successResponse)\r\n              this.resultdata = successResponse.data[0]\r\n              this.ruleForm.stu_id = this.resultdata.passedUsername\r\n              this.ruleForm.name = this.resultdata.name\r\n              this.ruleForm.sex = this.resultdata.gender\r\n              this.ruleForm.valueProvinceCity = this.resultdata.workplace\r\n              this.ruleForm.education = this.resultdata.degree\r\n              this.ruleForm.phonenum = this.resultdata.phone\r\n              this.ruleForm.email = this.resultdata.email\r\n            }\r\n        )\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.body {\r\n  width: 1000px;\r\n  margin-right: auto;\r\n  margin-left: auto;\r\n  margin-top: 15px;\r\n}\r\n\r\n.tit {\r\n  margin-right: auto;\r\n  margin-left: auto;\r\n  width: 70px;\r\n  height: 30px;\r\n  line-height: 30px;\r\n  text-align: center;\r\n  background: #f4f4f4;\r\n  border-radius: 3px;\r\n  font-size: 12px;\r\n  color: #697180;\r\n  letter-spacing: 0.4px;\r\n}\r\n\r\n.body {\r\n  width: 1000px;\r\n  margin-right: auto;\r\n  margin-left: auto;\r\n  margin-top: 15px;\r\n}\r\n\r\n.el-header {\r\n  width: 1000px;\r\n  color: #333;\r\n  text-align: left;\r\n  font-size: 13px;\r\n}\r\n\r\n.el-main {\r\n  color: #333;\r\n  text-align: center;\r\n  line-height: 160px;\r\n  border-top: 1px solid #f4f4f4;\r\n}\r\n\r\n.xz-tip {\r\n  width: 34px;\r\n  height: 20px;\r\n  line-height: 20px;\r\n  text-align: center;\r\n  font-size: 15px;\r\n  color: #697180;\r\n  letter-spacing: 0.37px;\r\n  background: #e9eaeb;\r\n  border-radius: 3px;\r\n  margin: 0 30px 0 10px;\r\n}\r\n\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 120px;\r\n  height: 120px;\r\n  line-height: 120px;\r\n  text-align: center;\r\n}\r\n\r\n.avatar {\r\n  width: 120px;\r\n  height: 120px;\r\n  display: block;\r\n}\r\n\r\n</style>\r\n"]}]}